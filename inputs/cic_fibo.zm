_fibo : int (int n){
  var i : int;
  var num1 : int;
  var num2 : int;
  var result : int;
  num1 = 0;
  num2 = 1;
  i = 2;

  if(n <= 1){
    if(n == 0){
      return 0;
    }
    else{
      return 1;
    }
  }
  else{
    while(i <= n){
      result = num1 + num2;
      num1 = num2;
      num2 = result;
      i = i + 1;
    }
  }
  return result;
}

main{
  var x[10] : int;
  write(_fibo(8));
}
